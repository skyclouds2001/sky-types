      "types": "./src/index.ts"
    }
  },
  "homepage": "https://github.com/skyclouds2001/sky-types#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skyclouds2001/sky-types.git"
  },
  "bugs": {
    "url": "https://github.com/skyclouds2001/sky-types/issues",
    "email": "skyclouds2001@163.com"
  },
  "author": {
    "name": "skyclouds2001",
    "email": "skyclouds2001@163.com",
    "url": "https://skyclouds2001.github.io/"
  },
  "contributors": [
    "skyclouds2001"
  ],
  "packageManager": "pnpm@8.10.0",
  "engines": {
    "node": ">=18.13.0",
    "npm": ">=9.6.0",
